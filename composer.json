{
    "name": "searchanise/logger",
    "description": "Monolog bootstrap with predefined formatter and registered error handler",
    "minimum-stability": "stable",
    "license": "proprietary",
    "authors": [
        {
            "name": "Oleg Shabashov",
            "email": "oshabashov@searchanise.io"
        }
    ],
    "config": {
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "Searchanise\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "tests\\": "tests"
        }
    },
    "scripts": {
        "test": "./vendor/bin/phpunit ./tests"
    },
    "require": {
        "php": "<8",
        "monolog/monolog": "^2.3"
    },
    "require-dev": {
        "ext-json": "*",
        "mikey179/vfsstream": "^1.6",
        "phpunit/phpunit": "^9.5",
        "roave/security-advisories": "dev-latest"
    }
}
